# challenge B: Stranger Tingz

use_bpm 160
use_synth :saw


# Sleep 4 beats 
live_loop :beat do
  play :c2
  sleep 0.5
  play :e2
  sleep 0.5
  play :g2
  sleep 0.5
  play :c4
  sleep 0.5
  play :b2
  sleep 0.5
  play :c3
  sleep 0.5
  play :b2
  sleep 0.5
  play :g2
  sleep 0.5
  play :e2
end

sleep 8

live_loop :trip do
  sample :ambi_dark_woosh
  sleep 2
  sample :ambi_swoosh
  sleep 2
end
live_loop :babyshark do
  sample :loop_electric, beat_stretch: 8
  sleep 8
end
